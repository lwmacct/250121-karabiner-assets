{
  "title": "lwm-cl-caps_lock",
  "rules": [
    {
      "description": "(lwm-cl-caps_lock) 必须导入这个",
      "manipulators": [
        {
          "description": "CapsLock 启动第一层",
          "type": "basic",
          "from": {
            "key_code": "caps_lock",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "parameters": {
            "basic.to_if_alone_timeout_milliseconds": 250,
            "basic.to_if_held_down_threshold_milliseconds": 1
          },
          "to_if_held_down": [
            {
              "set_variable": {
                "name": "lwm_caps_lock",
                "value": 1
              }
            }
          ],
          "to_after_key_up": [
            {
              "set_variable": {
                "name": "lwm_caps_lock",
                "value": 0
              }
            }
          ],
          "to_if_alone": [
            {
              "key_code": "caps_lock",
              "lazy": true
            }
          ]
        },
        {
          "description": "Space 启动第二层",
          "type": "basic",
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "parameters": {
            "basic.to_if_held_down_threshold_milliseconds": 1
          },
          "to_if_held_down": [
            {
              "set_variable": {
                "name": "lwm_caps_lock",
                "value": 2
              }
            }
          ],
          "to_after_key_up": [
            {
              "set_variable": {
                "name": "lwm_caps_lock",
                "value": 1
              }
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "lwm_caps_lock",
              "value": 1
            }
          ]
        }
      ]
    }
  ]
}
